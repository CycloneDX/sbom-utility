{
  "validation": {
    "actions": [
      {
        "id": "custom-metadata-properties-disclaimer-unique",
        "description": "Validate BOM metadata properties has a unique disclaimer value.",
        "selector": {
          "path": "metadata.properties",
          "primaryKey": {
            "key": "name",
            "value": "urn:example.com:disclaimer"
          }
        },
        "functions": [
          "isUnique"
        ]
      }
    ]
  }
}