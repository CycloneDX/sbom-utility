{
  "validation": {
    "actions": [
      {
        "id": "custom-bom-properties-primary-key-missing",
        "description": "Validate BOM metadata properties has a specific disclaimer value.",
        "selector": {
          "path": "metadata.properties",
          "primaryKey": {
            "key": "name",
            "value": "urn:example.com:disclaimer"
          }
        },
        "functions": [
          "hasProperties"
        ],
        "properties": [
          {
            "key": "name",
            "value": "urn:example.com:disclaimer"
          }
        ]
      }
    ]
  }
}